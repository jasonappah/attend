  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "build:icon": "tauri icon ./src-tauri/icons/icon.png",
    "build:tauri": "tauri build --debug",
    "build:web": "expo build",
    "data:reset": "docker compose down --volumes; rm /tmp/attend_replica.db",
    "dev": "expo start",
    "dev:clean": "expo start --clean",
    "dev:tauri": "tauri dev",
    "seed": "tsx ./src/postgres/seed.ts",
    "serve": "expo serve",
    "zero": "yarn zero:build-schema && zero-cache --schema-file ./src/zero/zero-schema.json --replica-file /tmp/attend_replica.db",
    "zero:dev": "zero-cache-dev -p ./src/zero/schema.ts --replica-file /tmp/attend_replica.db",
    "zero:build-schema": "cd ./src/zero && zero-build-schema -p './schema.ts'",
    "android": "expo run:android",
    "ios": "expo run:ios"
