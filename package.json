{
  "name": "attend",
  "version": "1.1.371",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "build:icon": "tauri icon ./src-tauri/icons/icon.png",
    "build:tauri": "tauri build --debug",
    "build:web": "one build",
    "migrate:gen": "drizzle-kit generate",
    "migrate:run": "drizzle-kit migrate",
    "data:reset": "docker compose down --volumes; rm /tmp/attend_replica.db",
    "dev": "one dev",
    "dev:clean": "one dev --clean",
    "dev:tauri": "tauri dev",
    "seed": "tsx ./src/postgres/seed.ts",
    "serve": "one serve",
    "zero:prod": "zero-cache --schema-file ./src/zero/zero-schema.json --replica-file /mnt/persist/attend_replica.db",
    "zero:dev": "zero-cache-dev -p ./src/zero/schema.ts --replica-file /tmp/attend_replica.db",
    "zero:build-schema": "cd ./src/zero && zero-build-schema -p './schema.ts'",
    "android": "one run:android",
    "ios": "one run:ios"
